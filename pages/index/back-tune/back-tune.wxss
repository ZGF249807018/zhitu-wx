.set-meal {
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: #f5f5f5;
  padding: 0 30rpx;
}

.set-meal .choice-meal {
  background-color: #fff;
  -webkit-box-shadow: 0px 10rpx 30rpx 0px rgba(0, 0, 0, 0.06);
  box-shadow: 0px 10rpx 30rpx 0px rgba(0, 0, 0, 0.06);
  border-radius: 16rpx;
  margin-bottom: 30rpx;
  margin-top: 30rpx;
}

.set-meal .choice-meal .choice-meal-title {
  font-size: 30rpx;
  height: 102rpx;
  line-height: 102rpx;
  padding-left: 30rpx;
  border-bottom: 1px solid #e0e0e0;
  color: #333;
}

.set-meal .choice-meal .choice-meal-main {
  color: #8e8e8e;
  font-size: 28rpx;
  text-align: center;
  padding: 50rpx 0;
}

.set-meal .choice-meal .choice-meal-main .more-small {
  margin-left: 20rpx;
  width: 11rpx;
  height: 20rpx;
}

.set-meal .choice-meal .choice-meal-main2 {
  font-size: 28rpx;
  padding: 30rpx;
  position: relative;
  min-height: 150rpx;
}

.noselect {
  display: flex;
  justify-content: center;
  text-align: center;
  color: #8e8e8e;
  font-size: 28rpx;
}

.noselect>image {
  margin-left: 20rpx;
  margin-top: 10rpx;
  width: 11rpx;
  height: 20rpx;
}

.set-meal .choice-meal .choice-meal-main2 .meal-name {
  margin-bottom: 20rpx;
}

.set-meal .choice-meal .choice-meal-main2 .meal-name text {
  color: #333;
  font-weight: 600;
}

.set-meal .choice-meal .choice-meal-main2 .meal-name .meal-price {
  margin-left: 20rpx;
  color: #f5402d;
}

.set-meal .choice-meal .choice-meal-main2 .meal-desc {
  color: #a3a3a3;
  margin-bottom: 20rpx;
}

.set-meal .choice-meal .choice-meal-main2 .meal-details {
  color: #2da0f4;
}

.set-meal .choice-meal .choice-meal-main2 .meal-more {
  width: 160rpx;
  height: 230rpx;
  line-height: 80rpx;
  position: absolute;
  right: 9rpx;
  top: 0%;
  margin-top: -8.5rpx;
}

.set-meal .choice-meal .choice-meal-main2 .meal-more image {
  width: 17rpx;
  height: 30rpx;
  position: absolute;
  right: 50rpx;
  top: 44%;
}

.set-meal .add-candidate {
  background-color: #fff;
  -webkit-box-shadow: 0px 10rpx 30rpx 0px rgba(0, 0, 0, 0.06);
  box-shadow: 0px 10rpx 30rpx 0px rgba(0, 0, 0, 0.06);
  border-radius: 16rpx;
}

.set-meal .add-candidate .add-candidate-header {
  padding: 0 30rpx;
  font-size: 30rpx;
  color: #333;
  height: 102rpx;
  line-height: 102rpx;
  border-bottom: 1px solid #e0e0e0;
}

.set-meal .add-candidate .add-candidate-header .add-candidate-icon {
  width: 50rpx;
  height: 50rpx !important;
  float: right;
  position: relative;
  top: 50%;
  margin-top: -25rpx;
}

.set-meal .add-candidate .add-candidate-main {
  color: #8e8e8e;
  font-size: 28rpx;
  text-align: center;
  padding: 50rpx 0;
}

.set-meal .add-candidate .add-candidate-main .more-small {
  margin-left: 20rpx;
  width: 11rpx;
  height: 20rpx;
}

.set-meal .add-candidate .add-candidate-main2 {
  text-align: left;
  color: #a3a3a3;
  padding: 30rpx;
  font-size: 28rpx;
}

.add-candidate-main2>view {
  display: inline-block;
  position: relative;
  margin-bottom: 30rpx;
  margin-right: 22rpx;
}

.set-meal .meal-function {
  background-color: #fff;
  -webkit-box-shadow: 0px 10rpx 30rpx 0px rgba(0, 0, 0, 0.06);
  box-shadow: 0px 10rpx 30rpx 0px rgba(0, 0, 0, 0.06);
  border-radius: 16rpx;
  margin-bottom: 30rpx;
  margin-top: 30rpx;
}

.set-meal .meal-function .coupon, .set-meal .meal-function .pay-mode,
.set-meal .meal-function .remarks {
  position: relative;
  font-size: 30rpx;
  height: 102rpx;
  line-height: 102rpx;
  padding-left: 30rpx;
  border-bottom: 1px solid #e0e0e0;
  color: #333;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -ms-flex-pack: justify;
  justify-content: space-between;
}

.remarks>text {
  display: inline-block;
  width: 150rpx;
}

.set-meal .meal-function .coupon .more-small,
.set-meal .meal-function .pay-mode .more-small,
.set-meal .meal-function .remarks .more-small {
  width: 16rpx;
  height: 30rpx !important;
  position: absolute;
  right: 50rpx;
  top: 50%;
  margin-top: -15rpx;
}

.set-meal .meal-function .coupon .coupon-num {
  position: absolute;
  right: 50rpx;
  top: 0;
  margin: 0;
  color: #2da0f4;
}

.set-meal .meal-function .coupon .coupon-num .more-small {
  position: static;
  right: 0;
  top: 0;
  margin: 0;
  margin-left: 30rpx;
}

.set-meal .meal-function .pay-mode .pay-items {
  margin-right: 50rpx;
}

.set-meal .meal-function .remarks {
  border-bottom: none;
  padding-right: 50rpx;
}

.set-meal .meal-function .remarks ._input {
  height: 100rpx;
  line-height: 100rpx;
  background-color: transparent;
  text-align: right;
}

.set-meal .customer-service {
  position: fixed;
  left: 30rpx;
  bottom: 120rpx;
  width: 186rpx;
  text-align: center;
}

.set-meal .customer-service image {
  width: 100%;
}

.set-meal .settlement {
  background-color: #fff;
  height: 100rpx;
  line-height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2;
  width: 100%;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -ms-flex-pack: justify;
  justify-content: space-between;
  font-size: 28rpx;
}

.set-meal .settlement .total {
  margin-left: 30rpx;
  color: #8e8e8e;
}

.set-meal .settlement .total text {
  font-size: 36rpx;
  color: #f5402d;
}

.set-meal .settlement .settlement-btn {
  border-radius: none;
}

.set-meal .settlement .settlement-btn text {
  display: block;
  text-align: center;
  width: 200rpx;
  height: 100rpx;
  line-height: 100rpx;
  color: #fff;
  background-color: #e0e0e0;
  border: 1px solid #e0e0e0;
  font-size: 28rpx;
  border-radius: none;
  padding: 0;
}

.set-meal .settlement .active text {
  background-color: #2da0f4;
}

.set-meal .meal-details-text {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 3;
}

.set-meal .meal-details-text .meal-pz {
  position: fixed;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  z-index: 3;
  background-color: rgba(0, 0, 0, 0.5);
}

.set-meal .meal-details-text .meal-details-box {
  position: fixed;
  bottom: 0;
  left: 0;
  background-color: #fff;
  width: 100%;
  min-height: 688rpx;
  font-size: 28rpx;
  z-index: 4;
  padding-bottom: 98rpx;
}

.set-meal .meal-details-text .meal-details-box .meal-details-header {
  text-align: center;
  color: #333;
  height: 100rpx;
  margin: 0 30rpx;
  line-height: 100rpx;
  border-bottom: 1px solid #e0e0e0;
  font-weight: bold;
}

.set-meal .meal-details-text .meal-details-box .meal-text-name {
  display: block;
  margin-left: 50rpx;
  margin-top: 28rpx;
  margin-bottom: 20rpx;
  color: #333;
}

.set-meal .meal-details-text .meal-details-box .meal-text-name text {
  display: inline-block;
  width: 52rpx;
  height: 52rpx;
  line-height: 52rpx;
  /* line-height: 52rpx; */
  border: 1px solid #2da0f4;
  border-radius: 50%;
  font-size: 24rpx;
  color: #2da0f4;
  /* padding: 10rpx; */
  text-align: center;
  box-sizing: border-box;
  margin-right: 20rpx;
}

.set-meal .meal-details-text .meal-details-box .meal-text-wenzi {
  display: block;
  line-height: 28rpx;
  margin: 0 50rpx;
}

.set-meal .meal-details-text .meal-details-box .meal-details-close {
  width: 100%;
  position: fixed;
  bottom: 0;
  border: none;
  background-color: #2da0f4;
  color: #fff;
  height: 98rpx;
  line-height: 98rpx;
  text-align: center;
  border-radius: 0;
}

.set-meal .meal-details-text .meal-details-box .coupon-list .coupon-item {
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  -ms-flex-pack: justify;
  justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center;
  text-align: left;
  color: #333;
  height: 100rpx;
  line-height: 100rpx;
  margin: 0 30rpx;
  border-bottom: 1px solid #e0e0e0;
  padding-left: 20rpx;
  padding-right: 20rpx;
}

.coupon-list {
  height: 555rpx;
  overflow: auto;
}

.contact {
  display: flex;
  justify-content: space-between;
  width: 186rpx;
  height: 70rpx;
  position: fixed;
  bottom: 120rpx;
  left: 30rpx;
  z-index: 2;
  background: #2da0f4;
  box-shadow: 0px 5rpx 15rpx 0px #2da0f4;
  border-radius: 35rpx;
  align-items: center;
}

.contact image {
  width: 50rpx;
  height: 50rpx;
  margin: 10rpx;
}

.contact>button {
  background: transparent;
  font-size: 24rpx;
  /* padding-right: 20rpx; *//* box-sizing: border-box; */
  color: white;
  padding: 0;
  margin-left: 70rpx;
}

.contact>button::after {
  border: none;
}

.preferentialprice {
  font-size: 28rpx;
  color: #8e8e8e;
  padding-left: 30rpx;
  box-sizing: border-box;
}

.pop {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
}

.pop>view:nth-of-type(1) {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: #000;
  opacity: 0.4;
  z-index: 3;
}

.paypop {
  width: 620rpx;
  height: 490rpx;
  background: #fff;
  position: fixed;
  z-index: 5;
  left: 50%;
  transform: translateX(-50%);
  top: 30%;
}

.paypop-header image {
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 25rpx;
  left: 25rpx;
}

.paypop-header {
  display: flex;
  position: relative;
  border-bottom: 1px solid #e0e0e0;
}

.paypop-header>view:nth-of-type(2) {
  font-size: 28rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  margin: 20rpx auto;
  color: #333;
}

.paypop-body {
  text-align: center;
}

.paypop-body>view:nth-of-type(1) {
  padding: 20rpx 27rpx 0 25rpx;
  box-sizing: border-box;
  color: #8e8e8e;
}

.paypop-body>view:nth-of-type(1)>view:nth-of-type(2) {
  margin-top: 10rpx;
  font-size: 48rpx;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: #333;
  border-bottom: 1px solid #e0e0e0;
}

.paypop-section {
  padding: 29rpx 27rpx 0rpx 25rpx;
  box-sizing: border-box;
  position: relative;
  font-size: 28rpx;
  color: #333;
}

.paypop-section>view {
  padding-bottom: 30rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #e0e0e0;
}

.paypop-section image {
  position: absolute;
  right: 51rpx;
  top: 34rpx;
  width: 16rpx;
  height: 30rpx;
}

.btn_pay {
  margin: 100rpx auto;
  width: 600rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 100rpx;
  background-color: #d3a95a;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
}

/* 支付密码css start */

.bg_layer {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 9998;
}

.input_main {
  position: fixed;
  left: 0;
  bottom: 500rpx;
  width: 100%;
  height: 394rpx;
  background-color: #fff;
  z-index: 9999;
}

.input_title {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 32rpx;
  border-bottom: 1rpx solid #e2e2e2;
}

.input_back {
  position: absolute;
  left: 0;
  top: 0;
  width: 80rpx;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.input_back text {
  width: 20rpx;
  height: 20rpx;
  background-color: white;
  border: 1rpx solid #aaa;
  border-width: 5rpx 0 0 5rpx;
  transform: rotate(-45deg);
}

.input_tip {
  margin: 30rpx;
  font-size: 24rpx;
  color: #888;
}

/* 密码掩码模拟 */

.input_row {
  width: 565rpx;
  margin: 0 auto;
  height: 85rpx;
  position: relative;
  display: flex;
  align-items: center;
  border: 1rpx solid #8e8e8e;
  border-radius: 20rpx;
  margin-top: 30rpx;
}

.input_row .pwd_item {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  border-right: 1rpx solid #e0e0e0;
  position: relative;
}

.pwd_item:nth-last-of-type(1) {
  border-right: 0;
}

.pwd_item text {
  width: 30rpx;
  height: 30rpx;
  border-radius: 30rpx;
  background-color: #555;
}

.forget_pwd {
  float: right;
  margin: 30rpx;
  width: 100rpx;
  text-align: right;
  font-size: 24rpx;
  color: #ff7800;
}

/* 文本输入框位置: 设置到左边隐藏 */

.input_control {
  position: relative;
  left: -300rpx;
  bottom: 0;
  width: 100rpx;
  height: 100rpx;
}

.uni-input {
  width: 140rpx;
  height: 100rpx;
}

.err {
  width: 50rpx;
  height: 50rpx;
  display: inline-block;
  position: absolute;
  top: -19rpx;
  right: -40rpx;
}

.err image {
  width: 20rpx;
  height: 20rpx;
}
